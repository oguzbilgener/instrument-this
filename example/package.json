{
    "name": "example",
    "version": "0.0.0",
    "description": "All-in-one example for instrument-this",
    "main": "build/index.js",
    "type": "module",
    "author": "Oguz Bilgener (oguz@bilgener.me)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/oguzbilgener/instrument-this.git"
    },
    "scripts": {
        "build": "tsc",
        "start": "node build/index.js",
        "clean": "rm -rf build",
        "test": "exit 1",
        "semantic-release": "semantic-release",
        "fmt": "prettier --write '@(src|test)/**/*.@(ts|tsx)'",
        "lint": "npm run lint:source && npm run lint:tests",
        "lint:source": "eslint --ext .ts --ext .tsx src",
        "lint:tests": "eslint --ext .ts --ext .tsx test"
    },
    "dependencies": {
        "@opentelemetry/api": "^0.18.0",
        "@opentelemetry/core": "^0.18.0",
        "@opentelemetry/exporter-collector": "^0.18.0",
        "@opentelemetry/exporter-collector-grpc": "^0.18.0",
        "@opentelemetry/exporter-collector-proto": "^0.18.0",
        "@opentelemetry/instrumentation-http": "^0.18.0",
        "@opentelemetry/metrics": "^0.18.0",
        "@opentelemetry/node": "^0.18.0",
        "@opentelemetry/tracing": "^0.18.0",
        "@types/koa": "^2.13.1",
        "@types/koa__router": "^8.0.4",
        "instrument-this": "file:///../",
        "koa": "^2.13.1",
        "@koa/router": "^10.0.0"
    },
    "devDependencies": {
        "@types/node": "^14.14.35",
        "esbuild": "^0.9.2",
        "eslint": "^7.22.0",
        "husky": "^5.1.3",
        "lint-staged": "^10.5.4",
        "typescript": "^4.2.3"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "eslint --ext ts --ext tsx"
        ],
        "test/**/*.{ts,tsx}": [
            "eslint --ext ts --ext tsx"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
